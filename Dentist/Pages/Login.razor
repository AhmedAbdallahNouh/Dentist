@page "/login"
@inject Account _account
<h3>Login</h3>
@foreach(var user in users)
{
    <h3>@user.UserName</h3>

}
@code {
    public List<IdentityUser> users { get; set; } = new List<IdentityUser>();
    protected override async Task OnInitializedAsync()
    {
        users = await _account.getAllUsers();
    }
}
